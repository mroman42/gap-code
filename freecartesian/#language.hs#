-- Objects:
--   Basics
--   1
--   A × B
--   A → B

-- Morphism Constructors
--   Basics
--   id {A}
--   unit {A}
--   fst {A}{B}
--   snd {A}{B}
--   eval {A}{B}
--   f ∘ g
--   <f,g>
--   curry {C}{A}{B}

-- Equations
--   f = t,    for f : A → 1
--   fst ∘ (f,g) = g
--   snd ∘ (f,g) = f
--   (fst ∘ h , snd ∘ h) = h
--   eval ∘ <curry f , id> = f
--   curry (eval ∘ (h,id)) = h


-- Rules
--   f > t,   for f : A → 1
--   f ∘ id > f
--   id ∘ f > f
--



